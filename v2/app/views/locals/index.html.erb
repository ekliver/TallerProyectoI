
<br> 
<h2>Locals</h2>

<table>
  <thead>
    <tr>
      <th>Nombre Local</th>
      <th>Número Latitud</th>
      <th>Número Longitud</th>
      <th>Dirección Local</th>
      <th>Nombre Distrito</th>
      <th>Número Telefono</th>
      <th>Abierto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locals.each do |local| %>
      <tr>
        <td><%= local.nomLocal %></td>
        <td><%= local.numLatitud %></td>
        <td><%= local.numLongitud %></td>
        <td><%= local.dirLocal %></td>
        <td><%= local.nomDistrito %></td>
        <td><%= local.numTelefono %></td>
        <td><%= local.inAbierto %></td>
        <td><%= link_to 'Show', local %></td>
        <td><%= link_to 'Edit', edit_local_path(local) %></td>
        <td><%= link_to 'Destroy', local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Local', new_local_path %>
